{
	"name": "webwhatsapp",
	"version": "1.0.4-rc",
	"license": "MIT",
	"description": "This is a Whatsapp clone to demonstrate my Angular frontend skills.",
	"keywords": [
		"thiagosaud",
		"thiago saud",
		"whatsapp",
		"whatsapp web"
	],
	"author": {
		"name": "THIAGO DE BONIS CARVALHO SAAD SAUD",
		"url": "https://www.linkedin.com/in/thiagosaud/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/thiagosaud/webwhatsapp.git"
	},
	"scripts": {
		"ng": "ng",
		"start": "export NODE_OPTIONS=--openssl-legacy-provider; vercel dev",
		"build": "export NODE_OPTIONS=--openssl-legacy-provider; ng build --aot true",
		"test": "export NODE_OPTIONS=--openssl-legacy-provider; ng test",
		"e2e": "export NODE_OPTIONS=--openssl-legacy-provider; ng e2e",
		"lint": "ng lint",
		"commit": "git-cz",
		"commitlint:check": "commitlint --edit",
		"branchnamelint:check": "branch-name-lint .branchnamelint.json",
		"prettier:check": "pretty-quick --staged",
		"prettier:format": "pretty-quick --write \"{,!(node_modules)/**/}*.{ts, html, json, scss}\"",
		"prepare": "npx husky install"
	},
	"config": {
		"commitizen": {
			"path": "git-cz"
		}
	},
	"dependencies": {
		"@angular/animations": "~9.1.0",
		"@angular/common": "~9.1.0",
		"@angular/compiler": "~9.1.0",
		"@angular/core": "~9.1.0",
		"@angular/forms": "~9.1.0",
		"@angular/platform-browser": "~9.1.0",
		"@angular/platform-browser-dynamic": "~9.1.0",
		"@angular/router": "~9.1.0",
		"@ngrx/effects": "^9.1.2",
		"@ngrx/entity": "^9.1.2",
		"@ngrx/store": "^9.1.2",
		"@ngrx/store-devtools": "^9.1.2",
		"rxjs": "~6.5.4",
		"tslib": "^1.10.0",
		"uuid": "^8.1.0",
		"vercel": "^22.0.1",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.901.8",
		"@angular/cli": "^9.1.8",
		"@angular/compiler-cli": "~9.1.0",
		"@angular/language-service": "~9.1.0",
		"@commitlint/cli": "^17.4.0",
		"@commitlint/config-conventional": "^17.4.0",
		"@types/jasmine": "~3.5.0",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "^12.11.1",
		"@types/uuid": "^8.0.0",
		"branch-name-lint": "^2.1.1",
		"codelyzer": "^5.1.2",
		"commitizen": "^4.2.6",
		"git-cz": "^4.9.0",
		"husky": "^8.0.0",
		"jasmine-core": "~3.5.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "^5.1.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~2.1.0",
		"karma-jasmine": "~3.0.1",
		"karma-jasmine-html-reporter": "^1.4.2",
		"prettier": "^2.0.5",
		"pretty-quick": "^2.0.1",
		"protractor": "^7.0.0",
		"ts-node": "~8.3.0",
		"tslint": "~6.1.0",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "~3.8.3"
	}
}
